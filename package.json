{
  "name": "memento-mori",
  "version": "1.0.0",
  "description": "A life calendar following the memento mori concept.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "lint": "eslint src/**/*.ts main.ts",
    "lint:fix": "eslint src/**/*.ts main.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"main.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"main.ts\""
  },
  "keywords": [
    "obsidian",
    "plugin",
    "life-calendar",
    "memento-mori",
    "productivity",
    "time-tracking"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.10",
    "obsidian": "latest",
    "prettier": "^3.7.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
